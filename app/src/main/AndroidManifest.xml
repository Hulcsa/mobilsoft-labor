<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mobsoft.hulcsa.com.festivalapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:name=".FestivalAppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.events.EventsActivity" />
        <activity android:name=".ui.eventdetails.EventDetailsActivity" />
        <activity android:name=".ui.favourites.FavouritesActivity"/>
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="93525dec63a5bd8feca2fb8af2e5cdd731399604" />
    </application>

</manifest>